{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Video Demo 2D",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/video_demo.py",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "movenet_lightning",
                "--video_path",
                "${workspaceFolder}/videos/video_2025-11-15_01-38-21.mp4",
                "--output_path",
                "${workspaceFolder}/videos/video_2025-11-15_01-38-21_pose2D.mp4"
            ]
        },
        {
            "name": "Run Video Demo 3D",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/video_demo_3d.py",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "movenet_lightning",
                "--video_path",
                "${workspaceFolder}/videos/video_2025-11-15_01-38-21.mp4",
                "--output_path",
                "${workspaceFolder}/videos/video_2025-11-15_01-38-21_pose3D.mp4"
            ]
        }
    ]
}